{
    "name": "dnwjn/nova-button",
    "description": "A Laravel Nova package for adding buttons to your resources.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/dnwjn/nova-button",
    "license": "MIT",
    "authors": [
        {
            "name": "Danique Wijnalda",
            "email": "hello@dnwjn.dev",
            "homepage": "https://dnwjn.dev",
            "role": "Developer"
        },
        {
            "name": "Brian Dillingham",
            "email": "bdillingham88@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "laravel/nova": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dnwjn\\NovaButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dnwjn\\NovaButton\\NovaButtonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
